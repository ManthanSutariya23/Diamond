@model admin_labgrown.Models.TblRegister

@{
    ViewData["Title"] = "Details";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <h1 class="card-title ml10">User Detail</h1>
            <div class="col-md-4"></div>
            <div class="col-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body text-center">
                        <div class="mb-3">
                            <span style="font-weight: bold">Name: </span>
                            <span>@Html.DisplayFor(model => model.FName) @Html.DisplayFor(model => model.LName)</span>
                        </div>

                        <div class="mb-3">
                            <span style="font-weight: bold">Email: </span>
                            <span>@Html.DisplayFor(model => model.Email)</span>
                        </div>

                        <div class="mb-3">
                            <span style="font-weight: bold">Address: </span>
                            <span>@Html.DisplayFor(model => model.Address1) @Html.DisplayFor(model => model.Address2)</span>
                        </div>

                        <div class="mb-3">
                            <span style="font-weight: bold">Postcode: </span>
                            <span>@Html.DisplayFor(model => model.Postcode)</span>
                        </div>

                        <div class="mb-3">
                            <span style="font-weight: bold">City: </span>
                            <span>@Html.DisplayFor(model => model.City)</span>
                        </div>

                        <div class="mb-3">
                            <span style="font-weight: bold">State: </span>
                            <span>@Html.DisplayFor(model => model.State)</span>
                        </div>

                        <div class="mb-3">
                            <span style="font-weight: bold">Country: </span>
                            <span>@Html.DisplayFor(model => model.Country.CountryName)</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4"></div>
        </div>
    </div>
</div>


